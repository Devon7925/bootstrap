        let const_size: i32 = 1 + leb_i32_len(0);
        return const_size + 1 + leb_u32_len(branch_depth);
    };
    if kind == 24 {
        let branch_depth: i32 = load_i32(entry_ptr + 4);
        if branch_depth < 0 {
            return -1;
        };
        return 1 + leb_u32_len(branch_depth);
