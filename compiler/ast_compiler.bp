        };
        let right_size: i32 = expression_code_size(ast_base, right_index);
        if right_size < 0 {
            return -1;
        };
        return left_size + right_size + 6;
    };
    if kind == 22 {
        let value_index: i32 = load_i32(entry_ptr + 4);
        let value_size: i32 = expression_code_size(ast_base, value_index);
        if value_size < 0 {
            return -1;
        };
